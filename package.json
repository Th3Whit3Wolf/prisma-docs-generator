{
  "name": "prisma-docs-generator",
  "version": "0.0.3",
  "main": "dist/index.js",
  "license": "MIT",
  "bin": {
    "prisma-docs-generator": "dist/cli.js"
  },
  "description": "Documentation Generator for Prisma Schema",
  "dependencies": {
    "@prisma/generator-helper": "^2.2.0",
    "@prisma/sdk": "^2.3.0",
    "express": "^4.17.1",
    "indent-string": "^4.0.0",
    "kleur": "^4.0.2",
    "meow": "^7.0.1",
    "pluralize": "^8.0.0",
    "prismjs": "^1.20.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.3.0-dev.9",
    "@prisma/client": "^2.3.0-dev.9",
    "@types/express": "^4.17.7",
    "@types/node": "^13.11.1",
    "@types/prismjs": "^1.16.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "ts-node main.ts",
    "generate": "prisma generate",
    "clean": "rm -rf dist && rm -rf styles/dist",
    "build:gen": "tsc -d && cp -r src/styles dist",
    "build:styles": "cd theme && yarn build && cp ./dist/main.css ../src/styles",
    "build": "yarn build:styles && yarn build:gen"
  }
}
